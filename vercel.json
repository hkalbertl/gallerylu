{
  "rewrites": [
    {
      "source": "/proxy/:subdomain/:path*",
      "destination": "https://:subdomain.filelu.live/:path*",
      "has": [
        {
          "type": "query",
          "key": "bypass"
        }
      ]
    },
    {
      "source": "/config",
      "destination": "/"
    },
    {
      "source": "/gallery(.*)",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
        }
      ]
    }
  ]
}
